<?xml version="1.0"?>
<robot name="rick">

<!-- White Material -->
    <material name="white">
        <color rgba="1 1 1 1"/>
    </material>
    <!-- Black Material -->
    <material name="black">
        <color rgba="0 0 0 1"/>
    </material>

    <material name="blue">
        <color rgba="0 0 0.8 1" />
      </material>
<!-- links -->
<!-- base_link -->
  <link name="base_link">
    <visual>
      <geometry>
        <cylinder length="0.155" radius="0.178" />
      </geometry>
      <material name="blue">
        <color rgba="0 0 0.8 1" />
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.155" radius="0.178" />
      </geometry>
    </collision>
    <inertial>
      <mass value="12" />
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.11908" ixy="0.0" ixz="0.0" iyy="0.11908" iyz="0.0" izz="0.1901" />
    </inertial>
  </link>

