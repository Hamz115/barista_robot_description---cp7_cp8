      <origin rpy="1.570796  0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder radius="0.0352" length="0.0206"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin rpy="1.570796  0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.0352" radius="0.0206" />
      </geometry>
    </collision>
    <inertial>
            <mass value="0.25"/>
            <origin rpy="1.570796 0 0" xyz="0 0 0"/>
            <inertia ixx="0.000058" ixy="0" ixz="0" iyy="0.000058" iyz="0" izz="0.000050"/>
        </inertial>
  </link>

<!-- left-wheel -->
  <link name="left_wheel">
    <visual>
      <origin rpy="1.570796 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder radius="0.0352" length="0.0206"/>
