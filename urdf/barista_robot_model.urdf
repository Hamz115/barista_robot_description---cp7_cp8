<!-- front_caster -->
  <link name="front_caster">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.020"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.020"/>
      </geometry>
    </collision>
    <inertial>
            <mass value="0.25"/>
            <origin rpy="0 0 0" xyz="0 0 0"/>
